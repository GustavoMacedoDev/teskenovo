	<queryString language="SQL">
		<![CDATA[SELECT 
  mesa.numero_mesa, produto.produto, produto.valor_produto, 
  lancamento.id_lancamento, quantidade.quantidade
FROM 
  public.lancamento, 
  public.produto, 
  public.mesa, 
  public.quantidade
WHERE 
  lancamento.mesa_mesa_id = mesa.mesa_id AND
  lancamento.produto_id_produto = produto.id_produto AND
